{
  "ConnectionsStrings": {
    "DefaultConnection": "server=localhost;user id=root;database=ent_facilities;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",

  "spring": {
    "application": {
      "name": "EntCloud"
    }
  },
  "eureka": {
    "client": {
      "shouldRegisterWithEureka": true,
      "serviceUrl": "http://localhost:8761/eureka",
      "ValidateCertificates": false
    },
    "instance": {
      "appName": "EntCloud",
      "hostName": "localhost",
      "port": "5040"
    }
  }
}
